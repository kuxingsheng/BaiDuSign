{
	"name": "BaiDuSign",
	"description": "BaiDuSign",
	"version": "1.1",
	"manifest_version": 2,
	"permissions": [
	"notifications",
	"tabs",
	"webRequest",
	"webRequestBlocking",
	"*://*/*"],
	"content_security_policy": "script-src 'self' 'unsafe-eval' 'unsafe-inline'; object-src 'self'",
	"background": {
		"scripts": ["background.js"]
		},
	"browser_action": {
		"default_icon" : "icon.png",
		"default_title": "BaiDuSign"
	}
}